{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Function": "Warning",
      "Host": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "applicationInsights": {
      "logLevel": {
        "Default": "Information",
        "Azure": "Warning",
        "Function": "Warning",
        "Host": "Warning",
        "Microsoft": "Warning",
        "System": "Warning"
      },
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception"
      }
    }
  },
  "extensions": {
    "serviceBus": {
      "maxConcurrentCalls": 1,
      "messageHandlerOptions": {
        "maxConcurrentCalls": 1
      }
    }
  }
}